    "react-hook-form": "^7.61.1",
    "react-i18next": "^13.5.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^2.1.9",
    "react-router-dom": "^6.30.1",
    "react-select": "^5.10.2",
    "react-spring": "^10.0.1",
    "recharts": "^2.15.4",
