{
  "name": "amora-pwa",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:dev": "vite build --mode development",
    "build:pwa": "npm run generate-icons && vite build",
    "generate-icons": "node scripts/generate-icons.js",
    "update-pwa-icons": "node scripts/update-pwa-icons.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "pwa:test": "npm run build:pwa && npm run preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:watch": "vitest --coverage --watch",
    "test:watch": "vitest --watch",
    "test:critical": "vitest run src/__tests__/hooks/useAuth.test.ts src/__tests__/hooks/useFeed.test.ts",
    "test:components": "vitest run src/__tests__/components/",
    "test:utils": "vitest run src/__tests__/utils/",
    "test:integration": "vitest run src/__tests__/integration/",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:mobile": "playwright test --project=mobile-chrome",
    "test:e2e:auth": "playwright test e2e/auth.spec.ts",
    "test:e2e:feed": "playwright test e2e/feed.spec.ts",
    "test:e2e:matching": "playwright test e2e/matching.spec.ts",
    "test:e2e:pwa": "playwright test e2e/pwa.spec.ts",
    "test:e2e:performance": "playwright test e2e/performance.spec.ts",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "test:all": "npm run test:run && npm run test:e2e",
    "test:ci": "npm run lint && npm run type-check && npm run test:coverage && npm run test:e2e",
    "type-check": "tsc --noEmit",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "health-check": "node scripts/health-check.js",
    "deployment-check": "node scripts/deployment-check.js",
    "audit:security": "npm audit --audit-level high",
    "audit:fix": "npm audit fix",
    "clean": "rm -rf dist node_modules/.vite",
    "deps:update": "npx npm-check-updates -u",
    "size-check": "npx bundlesize",
    "build:prod": "npm run cleanup:prod && npm run build",
    "cleanup:prod": "node scripts/production-cleanup.js",
    "preview:prod": "npm run build:prod && npm run preview"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm run test:run"
    }
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
