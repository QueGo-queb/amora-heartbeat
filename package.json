    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lighthouse-ci": "^1.10.0",
    "lint-staged": "^16.1.6",
    "lovable-tagger": "^1.1.9",
